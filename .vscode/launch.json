{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Expo: Start Dev Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/expo",
      "args": ["start", "-c"],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "name": "Expo: Open Web",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/expo",
      "args": ["start", "--web"],
      "console": "integratedTerminal"
    }
  ]
}








