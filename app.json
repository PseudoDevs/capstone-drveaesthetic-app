{
  "expo": {
    "name": "Dr. Ve Aesthetic App",
    "slug": "capstone-aesthetic-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/clinic-logo.jpg",
    "scheme": "capstone-aesthetic-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/clinic-logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/clinic-logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "capstone.aesthetic.app",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/images/clinic-logo.jpg",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "{{unread_count}} new messages"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/clinic-logo.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/clinic-logo.jpg",
          "color": "#000000",
          "sounds": ["./assets/sounds/notification.wav"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0ef114e3-7926-46d0-8936-d0b84c90612f"
      }
    }
  }
}
